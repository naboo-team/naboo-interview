{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "start": "next start -p 3001",
    "build": "next build",
  
    "check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",

    "test": "vitest",

    "generate-types": "cp ../back-end/schema.gql ./src/graphql/schema.gql && graphql-codegen",
    "docker:generate-types": "graphql-codegen --config codegen.docker.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@mantine/core": "^6.0.16",
    "@mantine/form": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@mantine/next": "^6.0.16",
    "@tabler/icons-react": "^2.25.0",
    "axios": "^1.4.0",
    "graphql-tag": "^2.12.6",
    "mongoose": "8.9.5",
    "next": "^13.5.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint-config-next": "13.4.10",
    "jsdom": "^22.1.0",
    "vitest": "^0.33.0"
  }
}
